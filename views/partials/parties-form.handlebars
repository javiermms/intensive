<h1>Start a Search Party Form</h1>
<br>

<fieldset>
  <div class="form-group">
    <label for="title">Title:</label>
    <input class="form-control" id="title" type="text">
  </div>
  <div class="form-group">
    <label for="location">Location:</label>
    <input class="form-control" id="location" type="text" >
  </div>
  <div class="form-group">
    <label for="time">Time:</label>
    <input class="form-control" id="time" type="text">
  </div>
  <div class="form-group">
    <label for="details">Details on Disapperance:</label>
    <textarea class="form-control" id="details" rows="3"></textarea>
  </div>
</fieldset>
